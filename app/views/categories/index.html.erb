<div class="container">
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2">
      <div id="categories">
        <% @categories.each do |category| %>
          <h4><%= category.name %></h4>
          <%= link_to category.name, category_path(category) %>
        <% end %>
      </div>
    </div>
  </div>
</div>
